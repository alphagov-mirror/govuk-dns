<% records['records'].each_with_index do | record, index | %>
resource "google_dns_record_set" "<%= record['subdomain'].gsub('.', '_').gsub('@', 'AT') %>_<%= index %>" {
  managed_zone = "<%= ENV['GOOGLE_MANAGED_ZONE'] %>"
  name = "<%= record['subdomain'] %>"
  type = "<%= record['record_type'] %>"
  ttl = "<%= record['ttl'] %>"
  rrdatas = ["<%= record['data'] %>"]
}
<% end %>
