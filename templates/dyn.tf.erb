<% records['records'].each_with_index do | record, index | %>
resource "dyn_record" "<%= record['subdomain'] %>_<%= index %>" {
   zone = "<%= ENV['DYN_ZONE_ID'] %>"
   name = "<%= record['subdomain'] %>"
   type = "<%= record['record_type'] %>"
   ttl = <%= record['ttl'] %>
   value = "<%= record['data'] %>"
}
<% end %>
