<% records['records'].each do | record | %>
resource "aws_route53_record" "<%= record['resource_title'] %>" {
  zone_id = "<%= ENV['ROUTE53_ZONE_ID'] %>"
  name = "<%= record['subdomain'] %>"
  type = "<%= record['record_type'] %>"
  ttl = "<%= record['ttl'] %>"
  records = ["<%= record['data'] %>"]
}
<% end %>
